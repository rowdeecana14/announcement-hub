{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": ["*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@store/*": ["src/store/*"],
      "@validations/*": ["src/validations/*"],
    }
  },
  "exclude": ["node_modules", "dist"]
}
